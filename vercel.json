{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/files/download",
      "dest": "/api/files/download.js"
    }
  ],
  "rewrites": [
    {
      "source": "/d",
      "destination": "/download.html"
    }
  ],
  "headers": [
    {
      "source": "/download.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}